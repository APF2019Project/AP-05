<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Scenes.SelectTypeOfCustom.SelectTypeOfCustomController">
   <children>
      <AnchorPane maxHeight="1.0" maxWidth="1.0" prefHeight="1.0" prefWidth="1.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fitHeight="489.0" fitWidth="863.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Files/plantsVsZombiesEmptyMiddle.jpg" />
               </image>
            </ImageView>
            <Button mnemonicParsing="false" onMouseClicked="#onBackButtonMouseClicked" opacity="0.88">
               <graphic>
                  <ImageView fitHeight="43.0" fitWidth="43.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Files/back.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="plantButton" layoutX="217.0" layoutY="188.0" mnemonicParsing="false" onMouseClicked="#onPlantButtonMouseClicked" prefHeight="55.0" prefWidth="201.0" text="Plant">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="zombieButton" layoutX="418.0" layoutY="188.0" mnemonicParsing="false" onMouseClicked="#onZombieButtonMouseClicked" prefHeight="55.0" prefWidth="201.0" text="Zombie">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="shieldButton" layoutX="217.0" layoutY="243.0" mnemonicParsing="false" onMouseClicked="#onShieldButtonMouseClicked" prefHeight="55.0" prefWidth="201.0" text="Shield">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="gunButton" layoutX="418.0" layoutY="243.0" mnemonicParsing="false" onMouseClicked="#onGunButtonMouseClicked" prefHeight="55.0" prefWidth="201.0" text="Gun">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
