<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fitHeight="489.0" fitWidth="863.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Files/81nvswO0GIL.png" />
               </image>
            </ImageView>
            <Button id="loginButton" layoutX="354.0" layoutY="389.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="155.0" text="Login">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <TextField id="usernameTextField" layoutX="301.0" layoutY="266.0" promptText="username">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <PasswordField id="passwordField" layoutX="301.0" layoutY="318.0" promptText="password">
               <font>
                  <Font size="20.0" />
               </font>
            </PasswordField>
            <Label id="titleLabel" fx:id="title" layoutX="184.0" layoutY="62.0" text="Plant Vs Zombies" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Ubuntu Light" size="65.0" />
               </font>
            </Label>
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="48.0" layoutY="155.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Files/pvzgw.0.jpg" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</VBox>
