                </ImageView>
                <Button fx:id="sendButton" layoutX="349.0" layoutY="407.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#onSendButtonMouseClicked" prefHeight="52.0" prefWidth="168.0">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <TextField fx:id="textField0" layoutX="301.0" layoutY="285.0">
                    <font>
                        <Font size="20.0" />
                    </font>
                </TextField>
                <PasswordField fx:id="textField1" layoutX="301.0" layoutY="340.0">
                    <font>
                        <Font size="20.0" />
                    </font>
                </PasswordField>
